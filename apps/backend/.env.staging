# Staging Environment Configuration
NODE_ENV=staging
PORT=3001

# MongoDB Connection
MONGODB_URI=mongodb://username:password@mongodb:27017/stock-management-staging?authSource=admin

# Financial Modeling Prep API
FMP_API_KEY=your_staging_api_key_here

# CORS Configuration
CORS_ORIGIN=https://staging.yourdomain.com

# Logging
LOG_LEVEL=debug

# Security
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=200
